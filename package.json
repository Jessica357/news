{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject"
  },
  "dependencies": {
    "expo": "^32.0.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz"
  },
  "devDependencies": {
    "@types/expo": "^31.0.8",
    "@types/jest": "^23.3.13",
    "@types/react": "^16.7.20",
    "@types/react-native": "^0.57.31",
    "@types/react-test-renderer": "^16.0.3",
    "babel-preset-expo": "^5.0.0",
    "jest": "^23.6.0",
    "jest-expo": "^32.0.0",
    "prettier": "^1.16.1",
    "prettier-tslint": "^0.4.2",
    "react-native-testing-library": "^1.5.0",
    "react-test-renderer": "^16.7.0",
    "ts-jest": "^23.10.5",
    "tsconfig-kodefox": "^1.0.1",
    "tslint": "^5.12.1",
    "tslint-config-kodefox": "^1.1.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.2.4"
  },
  "private": true,
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)+(spec|test).ts?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "ios.ts",
      "ios.tsx",
      "android.ts",
      "android.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    }
  }
}
